{
    "name": "raphael-thibierge/sir-edgar",
    "description": "Personal virtual assistant ",
    "keywords": ["assistant", "bot", "productivity"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-gd": "*",
        "ext-mongodb": "*",
        "ext-pcntl": "*",
        "ext-tokenizer": "*",
        "ext-zip": "*",
        "algolia/algoliasearch-client-php": "^1.25",
        "botman/botman": "^2.3",
        "botman/driver-facebook": "^1.6",
        "coinbase/coinbase": "^2.5",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "jenssegers/mongodb": "^3.7",
        "laravel/framework": "^7.29",
        "laravel/horizon": "^4.0",
        "laravel/passport": "^9.3.2",
        "laravel/scout": "^8.6",
        "laravel/tinker": "^2.6",
        "laravel/ui": "^2.0",
        "maatwebsite/excel": "^3.0",
        "openclerk/coinbase-oauth2": "^0.1.0",
        "pusher/pusher-php-server": "~3.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.6.1",
        "facade/ignition": "^2.0",
        "filp/whoops": "~2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
